<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false" android:state_active="false">
        <shape android:shape="rectangle">
            <corners android:radius="60dp"/>
            <stroke android:color="#FFFFFF" android:width="1dp"/>
        </shape>
    </item>
    <item android:state_pressed="true" android:state_active="true">
        <shape android:shape="rectangle">
            <corners android:radius="60dp"/>
            <solid android:color="#0367B4"/>
            <stroke android:color="#FFFFFF" android:width="1dp"/>
        </shape>
    </item>
    <item android:state_pressed="true" android:state_active="false">
        <shape android:shape="rectangle">
            <corners android:radius="60dp"/>
            <solid android:color="#0367B4"/>
            <stroke android:color="#FFFFFF" android:width="1dp"/>
        </shape>
    </item>
</selector>